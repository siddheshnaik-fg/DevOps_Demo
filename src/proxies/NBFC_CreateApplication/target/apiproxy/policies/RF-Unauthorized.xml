<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault async="false" continueOnError="false" enabled="true" name="RF-Unauthorized">
    <DisplayName>RF-Unauthorized</DisplayName>
    <Properties/>
    <FaultResponse>
        <Set>
            <Payload contentType="application/json">{"errorCode":"401","errorType":"Unauthorized","errorMessage":"You are not authorized."}</Payload>
            <StatusCode>401</StatusCode>
            <ReasonPhrase>Unauthorized</ReasonPhrase>
        </Set>
    </FaultResponse>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>