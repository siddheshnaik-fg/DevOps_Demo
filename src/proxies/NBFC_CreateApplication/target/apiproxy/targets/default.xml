<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<TargetEndpoint name="default">
    <Description/>
    <FaultRules/>
    <DefaultFaultRule name="PlatformFaults">
        <Step>
            <Name>AM-AddCORS</Name>
        </Step>
        <Step>
            <Name>RF-TimeOutError</Name>
            <Condition>fault.name = "ConnectionTimeout" or fault.name = "GatewayTimeout"</Condition>
        </Step>
        <Step>
            <Name>RF-InternalError</Name>
        </Step>
    </DefaultFaultRule>
    <PreFlow name="PreFlow">
        <Request>
            <Step>
                <Name>FC-GenerateSalesforceToken</Name>
            </Step>
            <Step>
                <Name>AS-SetRequest</Name>
            </Step>
        </Request>
        <Response/>
    </PreFlow>
    <PostFlow name="PostFlow">
        <Request/>
        <Response/>
    </PostFlow>
    <Flows/>
    <LocalTargetConnection>
        <APIProxy>NBFC_LOS_Target_Invocation</APIProxy>
        <ProxyEndpoint>default</ProxyEndpoint>
        <Path>/services/apexrest/createApplication</Path>
    </LocalTargetConnection>
</TargetEndpoint>