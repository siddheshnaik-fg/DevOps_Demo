<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AM-CreateOAuthRequest">
    <DisplayName>AM-CreateOAuthRequest</DisplayName>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="true" transport="https" type="request">myRequestHealth</AssignTo>
    <Add>
        <FormParams>
            <FormParam name="username">{private.username}</FormParam>
            <FormParam name="password">{private.password}</FormParam>
            <FormParam name="client_id">{private.client_id}</FormParam>
            <FormParam name="client_secret">{private.client_secret}</FormParam>
            <FormParam name="grant_type">password</FormParam>
        </FormParams>
    </Add>
</AssignMessage>