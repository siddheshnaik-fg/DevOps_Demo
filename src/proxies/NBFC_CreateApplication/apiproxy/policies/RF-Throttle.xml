<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault async="false" continueOnError="false" enabled="true" name="RF-Throttle">
    <DisplayName>RF-Throttle</DisplayName>
    <Properties/>
    <FaultResponse>
        <Set>
            <Payload contentType="application/json">{"errorCode":"429","errorType":"Too Many Requests","errorMessage":"You are sending too many requests."}</Payload>
            <StatusCode>429</StatusCode>
            <ReasonPhrase>Too Many Requests</ReasonPhrase>
        </Set>
    </FaultResponse>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>