<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ServiceCallout async="false" continueOnError="true" enabled="true" name="SC-GetAccessToken_Dev2">
    <DisplayName>SC-GetAccessToken_Dev2</DisplayName>
    <Properties/>
    <Request clearPayload="false" variable="myRequestHealth">
        <IgnoreUnresolvedVariables>false</IgnoreUnresolvedVariables>
    </Request>
    <Response>oauthResponseHealth</Response>
    <HTTPTargetConnection>
        <LoadBalancer>
            <Server name="NBFC_KVB_LOS_Auth"/>
        </LoadBalancer>
        <Path>/services/oauth2/token</Path>
        <SSLInfo>
            <Enabled>true</Enabled>
            <ClientAuthEnabled>false</ClientAuthEnabled>
        </SSLInfo>
        <Properties>
            <Property name="connect.timeout.millis">3000</Property>
            <Property name="io.timeout.millis">22000</Property>
        </Properties>
    </HTTPTargetConnection>
</ServiceCallout>